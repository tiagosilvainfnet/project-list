{
  "expo": {
    "name": "project-list",
    "slug": "project-list",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      ["expo-image-picker", { "photosPermission": "O app acessará suas fotos para que você possa escolhê-las ao seu gosto."}],
      ["expo-camera",
        {
          "cameraPermission": "Permitir $(PRODUCT_NAME) acessar sua câmera",
          "microphonePermission": "Permitir $(PRODUCT_NAME) acessar seu microfone",
          "recordAudioAndroid": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
